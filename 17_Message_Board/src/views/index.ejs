<html>
  <head>
    <link rel="stylesheet" href="styles.css"
  </head>
  <body>
  <h1>Message Board</h1>
  <%
    for (let i = 0; i < messages.length; i++) { %>
      <div class="message">
        <p class=text>
          <%= messages[i].text %>
        </p>
        <p class="user">
          <%= messages[i].user %>
        </p>
        <p class="added">
          <%= messages[i].added %>
        </p>
      </div>
  <% } %>
  </body>
</html>
